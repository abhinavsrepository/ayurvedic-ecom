# Development Environment Variables

# API Endpoints
API_URL=http://localhost:3333/api
ML_SERVICE_URL=http://localhost:5000/api/ml

# App Configuration
APP_ENV=development
APP_NAME=Ayurveda Haven Dev
APP_SCHEME=ayurveda

# Feature Flags
ENABLE_DEV_MENU=true
ENABLE_OFFLINE_MODE=true
ENABLE_ANALYTICS=false
ENABLE_CRASH_REPORTING=false

# Firebase (Development)
FIREBASE_API_KEY=your-dev-firebase-api-key
FIREBASE_AUTH_DOMAIN=your-dev-project.firebaseapp.com
FIREBASE_PROJECT_ID=your-dev-project-id
FIREBASE_STORAGE_BUCKET=your-dev-project.appspot.com
FIREBASE_MESSAGING_SENDER_ID=your-dev-sender-id
FIREBASE_APP_ID=your-dev-app-id

# OAuth (Development)
GOOGLE_OAUTH_CLIENT_ID=your-dev-google-client-id
GOOGLE_OAUTH_CLIENT_SECRET=your-dev-google-client-secret
APPLE_OAUTH_CLIENT_ID=your-dev-apple-client-id

# Payment Gateways (Test Mode)
RAZORPAY_KEY_ID=rzp_test_your_key_id
RAZORPAY_KEY_SECRET=your_test_key_secret
STRIPE_PUBLISHABLE_KEY=pk_test_your_publishable_key

# Sentry (optional in dev)
SENTRY_DSN=

# Other
LOG_LEVEL=debug
